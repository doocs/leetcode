{
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "phpVersion": "8.1",
    "braceStyle": "1tbs",
    "endOfLine": "lf",
    "sqlKeywordCase": "upper",
    "overrides": [
        {
            "files": ["*.sql"],
            "options": { "parser": "mysql" }
        }
    ],
    "plugins": ["prettier-plugin-sql-cst", "@prettier/plugin-php"]
}
